  "devDependencies": {
    "@humanfs/memory": "^0.19.5",
    "@humanfs/test": "^0.15.0",
    "@humanwhocodes/object-store": "^0.2.0",
    "@types/node": "^22.0.0",
    "mocha": "^10.2.0",
    "msw": "^2.3.0",
    "sinon": "^17.0.1",
    "typescript": "^5.2.2"
